<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blogspot.darokrithia.dungeonfungeon" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".RoomActivity" ></activity>
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreditsActivity" ></activity>
        <activity android:name=".StartActivity" ></activity>
        <activity android:name=".FightActivity"></activity>
        <activity android:name=".InventoryActivity"></activity>
        <activity android:name=".ItemSwitcherActivity"></activity>

    </application>

</manifest>
